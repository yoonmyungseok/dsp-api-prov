<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="go.kr.dsp.api.app.repository.DeployRepository">

  <resultMap id="Deploy" type="go.kr.dsp.api.app.domain.Deploy">
    <id property="id" column="id"/>
    <result property="host" column="host"/>
    <result property="seq" column="seq"/>
    <result property="service" column="service"/>
    <result property="inst" column="inst"/>
    <result property="dir" column="dir"/>
  </resultMap>

  <select id="selectService" resultMap="Deploy">
    SELECT host
    FROM test
    WHERE service=#{service} AND inst=#{inst} AND seq=#{seq}
  </select>

</mapper>